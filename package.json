    "http-server": "^0.10.0",
    "jest": "^22.4.3",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
